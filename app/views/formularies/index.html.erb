<% if can? :capture_formulary, @user %>
  <%= render 'capture_formulary'%>
<% elsif can? :view_state_results, @user %>
  <%= render 'state_results'%>
<% elsif can? :view_institutional_results, @user %>
  <%= render 'capture_institutional_formularies'%>
<% else %>
  <%= render 'generic_partial' %>
<% end %>
